<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <Description>Converts a YNAB-generated CSV export file into a plain text accounting ledger file compatible with ledger.</Description>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Authors>Michael Miceli</Authors>
    <PackageProjectUrl>https://github.com/vermiceli/ynab-to-ledger</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/vermiceli/ynab-to-ledger/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/vermiceli/ynab-to-ledger</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>ledger plaintextaccounting</PackageTags>
    <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="StyleCop.Cache" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="12.1.2" />
  </ItemGroup>

</Project>
